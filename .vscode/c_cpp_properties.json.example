{
	"configurations": [
		{
			"name": "Win32",
			"includePath": [
				"${workspaceFolder}/**",
				"${workspaceFolder}/src"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"compilerPath": "cl.exe",
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "windows-msvc-x64",
			"configurationProvider": "ms-vscode.cmake-tools"
		},
		{
			"name": "Mac",
			"includePath": [
				"${workspaceFolder}/**",
				"${workspaceFolder}/src"
			],
			"defines": [
				"_DEBUG",
				"UNICODE",
				"_UNICODE"
			],
			"macFrameworkPath": [
				"/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
			],
			"compilerPath": "/usr/bin/clang++",
			"cStandard": "c17",
			"cppStandard": "c++17",
			"intelliSenseMode": "macos-clang-x64",
			"configurationProvider": "ms-vscode.cmake-tools"
		}
	],
	"version": 4
}